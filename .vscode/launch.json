{
    "configurations": 
    [        
        {
            "name": ".NET Core Console",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Dotnet/Console/Learning_Console/bin/Debug/netcoreapp3.1/Learning_Console.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "requireExactSource": false
        },


        {
            "name": ".NET Core Web",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Dotnet/ASP/Learning_ASP/bin/Debug/netcoreapp3.1/Learning_ASP.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "requireExactSource": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "^\\s*Now listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"

            }
        },
    ]
}